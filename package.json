{
  "name": "caboose",
  "version": "1.0.0",
  "private": true,
  "main": "/src/main.ts",
  "repository": "https://github.com/CabooseMedia/caboose.git",
  "author": "Isaac Robbins <isaacprobbins@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "node ./dist/main.js",
    "dev": "ts-node-esm ./src/main.ts",
    "generate": "prisma generate --schema=./src/databases/prisma/sqlite.prisma",
    "migrate:dev": "prisma migrate dev --schema=./src/databases/prisma/sqlite.prisma",
    "migrate:deploy": "prisma migrate deploy --schema=./src/databases/prisma/sqlite.prisma",
    "studio": "prisma studio --schema=./src/databases/prisma/sqlite.prisma",
    "build": "tsc -p .",
    "test": "jest"
  },
  "dependencies": {
    "@prisma/client": "^5.3.1",
    "@types/module-alias": "^2.0.2",
    "chokidar": "^3.5.3",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "fs": "^0.0.1-security",
    "module-alias": "^2.2.3",
    "path": "^0.12.7",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "@babel/preset-typescript": "^7.22.5",
    "@jest/globals": "^29.6.3",
    "@types/express": "^4.17.17",
    "babel-jest": "^29.6.3",
    "jest": "^29.6.3",
    "prisma": "^5.3.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6"
  }
