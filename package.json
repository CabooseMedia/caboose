{
  "name": "caboose",
  "version": "1.0.0",
  "private": true,
  "main": "dist/index.js",
  "repository": "https://github.com/CabooseMedia/caboose.git",
  "author": "Isaac Robbins <isaacprobbins@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "cross-env CABOOSE_SERVER_ENV=production node dist/index.js",
    "dev": "cross-env CABOOSE_SERVER_ENV=development ts-node src/index.ts",
    "prisma:generate": "prisma generate --schema=./src/prisma/postgres.prisma",
    "prisma:migrate:dev": "prisma migrate dev --schema=./src/prisma/postgres.prisma",
    "prisma:migrate:deploy": "prisma migrate deploy --schema=./src/prisma/postgres.prisma",
    "build": "node_modules/.bin/rimraf dist && tsc -p .",
    "test": "jest"
  },
  "dependencies": {
    "@caboosemedia/sdk": "^0.0.1-beta3",
    "@prisma/client": "^5.7.1",
    "@types/module-alias": "^2.0.2",
    "chalk": "4.1.2",
    "chokidar": "^3.5.3",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "fdir": "^6.1.0",
    "fs": "^0.0.1-security",
    "module-alias": "^2.2.3",
    "path": "^0.12.7",
    "selfsigned": "^2.4.1",
    "socket.io": "^4.7.3",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "@babel/preset-typescript": "^7.22.5",
    "@jest/globals": "^29.6.3",
    "@types/express": "^4.17.17",
    "babel-jest": "^29.6.3",
    "copyfiles": "^2.4.1",
    "jest": "^29.6.3",
    "prisma": "^5.7.1",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  }
}
